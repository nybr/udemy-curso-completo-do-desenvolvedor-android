<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="atmempresaconsultoria.cursoandroid.com.atmempresaconsultoria">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EmpresaActivity"
            android:label="@string/empresa_detalhes" />
        <activity
            android:name=".ServicosActivity"
            android:label="@string/servicos_detalhes" />
        <activity
            android:name=".ClientesActivity"
            android:label="@string/clientes_detalhes" />
        <activity
            android:name=".ContatoActivity"
            android:label="@string/contatos_detalhes" />
    </application>

</manifest>